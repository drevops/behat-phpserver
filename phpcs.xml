<?xml version="1.0"?>
<ruleset name="behat-phpserver">
    <description>The coding standard for behat-phpserver.</description>

    <rule ref="Drupal">
        <exclude name="Drupal.Files.LineLength.TooLong"/>
    </rule>
    <rule ref="Generic.PHP.RequireStrictTypes"/>

    <file>src/</file>
    <file>tests/</file>

</ruleset>
